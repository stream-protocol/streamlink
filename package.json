{
    "name": "streamlink",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "// Setup Environments": "-------------------------------------------------------------",
        "env": "dotenv --",
        "env:dev": "dotenv -e .env.dev --",
        "write-env": "node scripts/env.js",
        "// CMS": "-------------------------------------------------------------",
        "prefetch": "node scripts/hygraph-prefetch.js",
        "prefetch:dev": "npm run env:dev -- npm run prefetch",
        "// Dev/Build": "-------------------------------------------------------------",
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "// Typescript": "-------------------------------------------------------------",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "// Lint": "-------------------------------------------------------------",
        "lint": "eslint ."
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^2.1.0",
        "@sveltejs/kit": "^1.25.2",
        "@tailwindcss/typography": "^0.5.10",
        "@typescript-eslint/eslint-plugin": "^5.47.1",
        "@typescript-eslint/parser": "^6.7.5",
        "autoprefixer": "^10.4.16",
        "daisyui": "^3.9.2",
        "dotenv-cli": "^7.3.0",
        "eslint": "^8.51.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-svelte": "^2.34.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "graphql-request": "^6.1.0",
        "postcss": "^8.4.31",
        "postcss-load-config": "^4.0.1",
        "svelte": "^4.2.1",
        "svelte-check": "^3.5.2",
        "svelte-preprocess": "^5.0.4",
        "tailwindcss": "^3.3.3",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "vite": "^4.4.11"
    },
    "type": "module",
    "dependencies": {
        "@solana/wallet-adapter-wallets": "^0.19.23",
        "@solana/web3.js": "^0.87.1",
        "@svelte-on-solana/wallet-adapter-core": "^1.0.11-alpha.0",
        "@svelte-on-solana/wallet-adapter-ui": "^1.0.21-alpha.0"
    }
}